			"TutorId": {
				"type": "string"
			},
			"StudentId": {
				"type": "string"
			},
			"UserId": {
				"anyOf": [
					{
						"$ref": "#/components/schemas/TutorId"
					},
					{
						"$ref": "#/components/schemas/StudentId"
					}
				]
			},
			"UrlString": {
				"type": "string",
				"description": "An internet link"
			},
			"LocaleCode": {
				"type": "string",
				"enum": [
					"en",
					"fr"
				]
			},
			"PublicStudentData": {
				"properties": {
					"id": {
						"$ref": "#/components/schemas/UserId",
						"description": "Unique id for a user"
					},
					"imageUrl": {
						"$ref": "#/components/schemas/UrlString",
						"description": "Link to profile image, if defined"
					},
					"introduction": {
						"type": "string",
						"description": "Optional text for users to introduce themselves,\r\ndescribe their experience, what subjects they are interested in etc"
					},
					"prefferedLocales": {
						"items": {
							"$ref": "#/components/schemas/LocaleCode"
						},
						"type": "array",
						"description": "The locales a user prefers for content"
					},
					"userName": {
						"type": "string",
						"description": "Username which will be displayed for a user"
					}
				},
				"required": [
					"id",
					"prefferedLocales",
					"userName"
				],
				"type": "object",
				"additionalProperties": false
			},
			"GetStudentsBySubjectsResponseBody": {
				"properties": {
					"students": {
						"items": {
							"$ref": "#/components/schemas/PublicStudentData"
						},
						"type": "array"
					}
				},
				"required": [
					"students"
				],
				"type": "object",
				"additionalProperties": false
			},
			"GenericSubjectId": {
				"type": "string"
			},
			"LocaleSubjectId": {
				"type": "string"
			},
			"SubjectId": {
				"anyOf": [
					{
						"$ref": "#/components/schemas/GenericSubjectId"
					},
					{
						"$ref": "#/components/schemas/LocaleSubjectId"
					}
				]
			},
			"GetStudentsBySubjectsRequestBody": {
				"properties": {
					"subjectIds": {
						"items": {
							"$ref": "#/components/schemas/SubjectId"
						},
						"type": "array"
					}
				},
				"required": [
					"subjectIds"
				],
				"type": "object",
				"additionalProperties": false
			},
			"FirebaseCallableFunctionContext": {
				"description": "Provided automatically by Firebase"
			},
			"PublicTutorData": {
				"properties": {
					"id": {
						"$ref": "#/components/schemas/UserId",
						"description": "Unique id for a user"
					},
					"imageUrl": {
						"$ref": "#/components/schemas/UrlString",
						"description": "Link to profile image, if defined"
					},
					"introduction": {
						"type": "string",
						"description": "Optional text for users to introduce themselves,\r\ndescribe their experience, what subjects they are interested in etc"
					},
					"prefferedLocales": {
						"items": {
							"$ref": "#/components/schemas/LocaleCode"
						},
						"type": "array",
						"description": "The locales a user prefers for content"
					},
					"userName": {
						"type": "string",
						"description": "Username which will be displayed for a user"
					},
					"available": {
						"type": "boolean",
						"description": "Represents whether a tutor is available to take on a student"
					}
				},
				"required": [
					"id",
					"prefferedLocales",
					"userName",
					"available"
				],
				"type": "object",
				"additionalProperties": false
			},
			"GetTutorsBySubjectsResponseBody": {
				"properties": {
					"tutors": {
						"items": {
							"$ref": "#/components/schemas/PublicTutorData"
						},
						"type": "array"
					}
				},
				"required": [
					"tutors"
				],
				"type": "object",
				"additionalProperties": false
