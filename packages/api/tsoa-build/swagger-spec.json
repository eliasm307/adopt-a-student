{
	"components": {
		"examples": {},
		"headers": {},
		"parameters": {},
		"requestBodies": {},
		"responses": {},
		"schemas": {
			"PublicStudentData": {
				"properties": {
					"id": {
						"type": "string",
						"description": "Unique id for a user"
					},
					"imageUrl": {
						"type": "string",
						"description": "Link to profile image, if defined"
					},
					"introduction": {
						"type": "string",
						"description": "Optional text for users to introduce themselves,\r\ndescribe their experience, what subjects they are interested in etc"
					},
					"prefferedLocales": {
						"items": {
							"type": "string"
						},
						"type": "array",
						"description": "The locales a user prefers for content"
					},
					"userName": {
						"type": "string",
						"description": "Username which will be displayed for a user"
					}
				},
				"required": [
					"id",
					"prefferedLocales",
					"userName"
				],
				"type": "object",
				"additionalProperties": false
			}
		},
		"securitySchemes": {}
	},
	"info": {
		"title": "@adopt-a-student/api",
		"version": "0.0.0",
		"description": "API for adopt-a-student",
