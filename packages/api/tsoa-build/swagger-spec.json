{
	"components": {
		"examples": {},
		"headers": {},
		"parameters": {},
		"requestBodies": {},
		"responses": {},
		"schemas": {
			"TutorId": {
				"type": "string"
			},
			"LinkedTutorData": {
				"description": "Data about a tutor from the perspective of a student, as it is stored in database",
				"properties": {
					"id": {
						"$ref": "#/components/schemas/TutorId",
						"description": "Id of the tutor related to the student"
					}
				},
				"required": [
					"id"
				],
				"type": "object",
				"additionalProperties": false
			},
			"EmailString": {
				"type": "string",
				"description": "A valid email address"
			},
			"LocaleSubjectId": {
				"type": "string"
			},
			"ConfidenceLevelEnum": {
				"description": "Confidence level options mapped to integers",
				"enum": [
					0,
					1,
					2,
					3
				],
				"type": "number"
			},
			"LocaleCode": {
				"type": "string",
				"enum": [
					"en",
					"fr"
				]
			},
			"UserSubjectData": {
				"description": "Data about a subject specific to a user, as it is stored in database\r\n// ? should this be public?",
				"properties": {
					"id": {
						"$ref": "#/components/schemas/LocaleSubjectId",
						"description": "Unique id to the related subject"
					},
					"confidenceLevel": {
						"$ref": "#/components/schemas/ConfidenceLevelEnum",
						"description": "A rating of how confident a user feels in the subject, the numbers"
					},
					"detail": {
						"type": "string",
						"description": "Detail of the users experience with the subject"
					},
					"locale": {
						"$ref": "#/components/schemas/LocaleCode"
					}
				},
				"required": [
					"id",
					"confidenceLevel",
					"detail",
					"locale"
				],
				"type": "object",
				"additionalProperties": false
			},
			"StudentId": {
				"type": "string"
			},
			"UserId": {
				"anyOf": [
					{
						"$ref": "#/components/schemas/TutorId"
					},
					{
						"$ref": "#/components/schemas/StudentId"
					}
				]
			},
			"UrlString": {
				"type": "string",
				"description": "An internet link"
			},
			"PrivateStudentData": {
				"description": "Schema of data that represents a student, as it is stored in database",
				"properties": {
					"id": {
						"$ref": "#/components/schemas/UserId",
						"description": "Unique id for a user"
					},
					"imageUrl": {
						"$ref": "#/components/schemas/UrlString",
						"description": "Link to profile image, if defined"
					},
					"introduction": {
						"type": "string",
						"description": "Optional text for users to introduce themselves,\r\ndescribe their experience, what subjects they are interested in etc"
					},
					"prefferedLocales": {
						"items": {
							"$ref": "#/components/schemas/LocaleCode"
						},
						"type": "array",
						"description": "The locales a user prefers for content"
					},
					"userName": {
						"type": "string",
						"description": "Username which will be displayed for a user"
					},
					"email": {
						"$ref": "#/components/schemas/EmailString",
						"description": "Email to allow users to start communicating"
					},
					"relatedSubjects": {
						"items": {
							"$ref": "#/components/schemas/UserSubjectData"
						},
						"type": "array",
						"description": "Links to subjects a user is interested in learning/teaching (depends on type of user)\r\nand user specific information about the user relating to a specific subject"
					},
					"relatedTutors": {
						"items": {
							"$ref": "#/components/schemas/LinkedTutorData"
						},
						"type": "array",
						"description": "Data about the tutors a student is involved with"
					}
				},
				"required": [
					"id",
					"prefferedLocales",
					"userName",
					"email",
					"relatedSubjects",
					"relatedTutors"
				],
				"type": "object",
				"additionalProperties": false
			},
			"CreateStudentResponseBody": {
				"properties": {
					"student": {
						"$ref": "#/components/schemas/PrivateStudentData"
					}
				},
				"required": [
					"student"
				],
				"type": "object",
				"additionalProperties": false
			},
			"Pick_PrivateStudentData.Exclude_keyofPrivateStudentData.id__": {
				"properties": {
					"relatedTutors": {
						"items": {
							"$ref": "#/components/schemas/LinkedTutorData"
						},
						"type": "array",
						"description": "Data about the tutors a student is involved with"
					},
					"email": {
						"$ref": "#/components/schemas/EmailString",
						"description": "Email to allow users to start communicating"
					},
					"relatedSubjects": {
						"items": {
							"$ref": "#/components/schemas/UserSubjectData"
						},
						"type": "array",
						"description": "Links to subjects a user is interested in learning/teaching (depends on type of user)\r\nand user specific information about the user relating to a specific subject"
					},
					"imageUrl": {
						"$ref": "#/components/schemas/UrlString",
						"description": "Link to profile image, if defined"
					},
					"introduction": {
						"type": "string",
						"description": "Optional text for users to introduce themselves,\r\ndescribe their experience, what subjects they are interested in etc"
					},
					"prefferedLocales": {
						"items": {
							"$ref": "#/components/schemas/LocaleCode"
						},
						"type": "array",
						"description": "The locales a user prefers for content"
					},
					"userName": {
						"type": "string",
						"description": "Username which will be displayed for a user"
					}
				},
				"required": [
					"relatedTutors",
					"email",
					"relatedSubjects",
					"prefferedLocales",
					"userName"
				],
				"type": "object",
				"description": "From T, pick a set of properties whose keys are in the union K"
			},
			"Omit_PrivateStudentData.id_": {
				"$ref": "#/components/schemas/Pick_PrivateStudentData.Exclude_keyofPrivateStudentData.id__",
				"description": "Construct a type with the properties of T except for those in type K."
			},
			"CreateStudentRequestBody": {
				"properties": {
					"student": {
						"$ref": "#/components/schemas/Omit_PrivateStudentData.id_"
					}
				},
				"required": [
					"student"
				],
				"type": "object",
				"additionalProperties": false
			},
			"FirebaseCallableFunctionContext": {
				"description": "Provided automatically by Firebase"
			},
			"PublicStudentData": {
				"properties": {
					"id": {
						"$ref": "#/components/schemas/UserId",
						"description": "Unique id for a user"
					},
					"imageUrl": {
						"$ref": "#/components/schemas/UrlString",
						"description": "Link to profile image, if defined"
					},
					"introduction": {
						"type": "string",
						"description": "Optional text for users to introduce themselves,\r\ndescribe their experience, what subjects they are interested in etc"
					},
					"prefferedLocales": {
						"items": {
							"$ref": "#/components/schemas/LocaleCode"
						},
						"type": "array",
						"description": "The locales a user prefers for content"
					},
					"userName": {
						"type": "string",
						"description": "Username which will be displayed for a user"
					}
				},
				"required": [
					"id",
					"prefferedLocales",
					"userName"
				],
				"type": "object",
				"additionalProperties": false
			},
			"GetStudentsBySubjectsResponseBody": {
				"properties": {
					"students": {
						"items": {
							"$ref": "#/components/schemas/PublicStudentData"
						},
						"type": "array"
					}
				},
				"required": [
					"students"
				],
				"type": "object",
				"additionalProperties": false
			},
			"GenericSubjectId": {
				"type": "string"
			},
			"SubjectId": {
				"anyOf": [
					{
						"$ref": "#/components/schemas/GenericSubjectId"
					},
					{
						"$ref": "#/components/schemas/LocaleSubjectId"
					}
				]
			},
			"GetStudentsBySubjectsRequestBody": {
				"properties": {
					"subjectIds": {
						"items": {
							"$ref": "#/components/schemas/SubjectId"
						},
						"type": "array"
					}
				},
				"required": [
					"subjectIds"
				],
				"type": "object",
				"additionalProperties": false
			}
		},
		"securitySchemes": {}
	},
