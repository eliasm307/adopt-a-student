{
  "name": "@adopt-a-student/web-app",
  "private": true,
  "description": "Web app for adopt a student",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "prebuild": "gatsby clean",
    "develop": "gatsby develop --open",
    "dev": "yarn start",
    "start": "yarn develop",
    "prestart": "yarn clean",
    "serve": "yarn build && gatsby serve",
    "clean": "rm -rf public && gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start-emulators": "yarn --cwd ../../ start-emulators"
  },
  "dependencies": {
    "gatsby": "^3.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "@adopt-a-student/common": "file:../api/common"
  },
  "devDependencies": {
    "@types/reach__router": "^1.3.7",
    "gatsby-plugin-tsconfig-paths": "^1.0.3",
    "prettier": "2.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}