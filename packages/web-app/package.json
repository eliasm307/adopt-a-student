{
  "name": "@adopt-a-student/web-app",
  "private": true,
  "description": "Web app for adopt a student",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "prebuild": "gatsby clean",
    "develop": "gatsby develop --open",
    "dev": "yarn start",
    "start": "yarn develop",
    "prestart": "yarn clean",
    "serve": "yarn build && gatsby serve",
    "clean": "rm -rf public && gatsby clean",
    "cleaninstall": "rm -rf node_modules && yarn clean && yarn",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start-emulators": "yarn --cwd ../../ start-emulators"
  },
  "dependencies": {
    "@adopt-a-student/common": "file:../api/common",
    "@types/react-query": "^1.1.2",
    "gatsby": "^3.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-query": "^3.13.11"
  },
  "devDependencies": {
    "@types/reach__router": "^1.3.7",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-imports": "^2.0.0",
    "babel-preset-gatsby": "^1.3.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-import-resolver-babel-module": "^5.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby-plugin-eslint": "^3.0.0",
    "gatsby-plugin-tsconfig-paths": "^1.0.3",
    "prettier": "2.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}